{
  "name": "vue-heroicons",
  "version": "0.0.1",
  "description": "Vue component to use SVG inline icons from the awesome Heroicons collection.",
  "keywords": [
    "heroicons",
    "vue",
    "icons",
    "iconpacks"
  ],
  "homepage": "https://github.com/d2pro/vue-heroicons",
  "bugs": {
    "url": "https://github.com/d2pro/vue-heroicons/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/d2pro/vue-heroicons"
  },
  "license": "MIT",
  "author": "Daniel Mu√±oz <daniel@d2pro.es>",
  "scripts": {
    "build": "node scripts/build.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "nuxt",
    "docs:build": "nuxt generate",
    "docs:preview": "cd docs/dist && http-server",
    "lint": "eslint --ext .js,.vue ./",
    "lint-fix": "eslint --fix --ext .js,.vue ./",
    "pub": "sh ./scripts/publish.sh",
    "start": "nuxt start",
    "submodule": "git submodule update --init",
    "update": "sh ./scripts/update.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --ext .js,.vue ./"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@nuxt/content": "^1.13.1",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "glob-promise": "^4.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "nuxt": "^2.15.2",
    "nuxt-i18n": "^6.20.3",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.49.0",
    "svgo": "^1.3.2",
    "vue-slider-component": "^3.2.11",
    "vue-swatches": "^2.1.0"
  }
}
